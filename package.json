{
  "name": "vite-plugin-xdm",
  "version": "0.1.0",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts src/frontmatter.ts --dts --sourcemap --format cjs,esm --no-splitting --target es2020",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.cjs",
    "prepublishOnly": "npm run build",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "lint": "eslint --ignore-pattern 'node_modules'  --ignore-pattern 'dist' . --ext .ts"
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./frontmatter": {
      "import": "./dist/frontmatter.js",
      "require": "./dist/frontmatter.cjs"
    }
  },
  "funding": "https://github.com/sponsors/ElMassimo",
  "author": "MÃ¡ximo Mussini <maximomussini@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/maximomussini/vite-plugin-xdm"
  },
  "homepage": "https://github.com/maximomussini/vite-plugin-xdm",
  "bugs": "https://github.com/maximomussini/vite-plugin-xdm/issues",
  "dependencies": {
    "@rollup/pluginutils": "^4.1.1",
    "js-yaml": "^4.1",
    "source-map": "^0.7.3",
    "toml": "^3.0"
  },
  "peerDependencies": {
    "vite": "^2.5.1"
  },
  "devDependencies": {
    "@mussi/eslint-config": "^0.5.1",
    "@types/estree": "^0.0.46",
    "@types/js-yaml": "^4.0.0",
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^7.32.0",
    "estree-util-is-identifier-name": "^1.1.0",
    "estree-util-value-to-estree": "^1.0.0",
    "execa": "^5.1.1",
    "minimist": "^1.2.5",
    "remark-frontmatter": "^4.0.0",
    "semver": "^7.3.5",
    "tsup": "^4.14.0",
    "typescript": "^4.1.3",
    "unified": "^10.1.0",
    "unist-util-visit": "^2.0.3",
    "vfile": "^5.1.0",
    "vite": "^2.5.1",
    "xdm": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "@mussi/eslint-config"
  }
}
